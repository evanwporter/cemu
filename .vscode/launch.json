  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Windows) Cemu",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug/cemu/cemu.exe",
      "args": [
        // "${workspaceFolder}/tests/gb-test-roms/cpu_instrs/cpu_instrs.gb"
        "${workspaceFolder}/tests/gb-test-roms/cpu_instrs/individual/01-special.gb"
        // "${workspaceFolder}/tests/Pokemon_Red.gb"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "integratedTerminal"
    },
    {
      "name": "(Windows) Vemu",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug/vemu/vemu.exe",
      "args": [
        // "${workspaceFolder}/tests/gb-test-roms/cpu_instrs/individual/01-special.gb",
        "${workspaceFolder}/tests/dmg-acid2.gb",
        // "--skip-boot-rom",
        "--gui"
        // "--trace"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "integratedTerminal"
    },
    {
      "name": "(Windows) Display PPM",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\build\\debug\\vemu\\tests\\util\\display_ppm.exe",
      "args": ["${workspaceFolder}\\vemu\\tests\\ppu\\golden\\bg_numbers.ppm"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "integratedTerminal"
    },
    {
      "name": "(Windows) Debugger",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug/vemu/debug.exe",
      "args": [
        // "${workspaceFolder}/tests/gb-test-roms/cpu_instrs/individual/01-special.gb"
        "${workspaceFolder}/tests/mooneye-test-suite/acceptance/oam_dma/basic.gb"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "integratedTerminal"
    }
  ]
}
